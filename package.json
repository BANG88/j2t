{
  "name": "j2t",
  "displayName": "j2t",
  "description": "Convert json to TypeScript interface.",
  "version": "0.0.2",
  "publisher": "bang",
  "icon": "images/icon.png",
  "homepage": "https://github.com/bang88/j2t/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bang88/j2t"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.convert.json.to.ts"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.convert.json.to.ts",
        "title": "Convert JSON to Interface"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}
